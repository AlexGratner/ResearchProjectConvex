@article{Behbahani2014,
abstract = {This paper presents a novel multimodal evolutionary$\backslash$noptimisation algorithm for the complex problem of$\backslash$nconcurrent and integrated design of a mechatronic$\backslash$nsystem, with the objective of realising the best$\backslash$ntopology and the best parameters from a multicriteria$\backslash$nviewpoint and with different preferences. The$\backslash$nassociated search space can be large and complex due to$\backslash$nthe existence of different classes of configurations,$\backslash$npossible topologies, and the parameter values of the$\backslash$nelements. The proposed algorithm efficiently explores$\backslash$nthe search space to find several elite configurations$\backslash$nfor different preferences, with more detailed$\backslash$ncompetition by incorporating the domain knowledge of$\backslash$nexperts and considering some criteria that are not$\backslash$nincluded in the course of regular evolutionary$\backslash$noptimisation. The developed approach consists of a$\backslash$ntwo-loop optimisation. For each topology, a genetic$\backslash$nalgorithm-based optimisation is performed to find an$\backslash$nelite representative of the topology. The elites will$\backslash$ncompete with each other to become the best design. A$\backslash$nstrategy of restricted competition selection is$\backslash$nemployed in the competition of topologies, with the aim$\backslash$nof finding alternative elites from which the one that$\backslash$nbest satisfies the customer preference may be chosen.$\backslash$nThe designer may incorporate a higher level competition$\backslash$nbetween elites in order to obtain the global optimum.},
author = {Behbahani, Saeed and {De Silva}, Clarence W.},
doi = {10.1109/TMECH.2012.2230013},
file = {:home/alexander/Documents/ResearchProject/Literature/Niching Genetic Scheme With Bond Graphs for Topology and Parameter Optimization of a Mechatronic System
.pdf:pdf},
isbn = {8415683111},
issn = {10834435},
journal = {IEEE/ASME Transactions on Mechatronics},
keywords = {Bond graphs (BGs),car suspension,genetic algorithms (GAs),genetic programming (GP),niching genetic schemes,skyhook},
number = {1},
pages = {269--277},
title = {{Niching genetic scheme with bond graphs for topology and parameter optimization of a mechatronic system}},
volume = {19},
year = {2014}
}
