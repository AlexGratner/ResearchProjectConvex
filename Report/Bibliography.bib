keywords = {Genetic algorithms (GAs),Mechatronic systems,System identification,Unknown structured system},
number = {1},
pages = {300--305},
title = {{GA-based evolutionary identification algorithm for unknown structured mechatronic systems}},
volume = {52},
year = {2005}
