abstract = {Optimizing a multidisciplinary integrated system design, taking into account multi-criteria interacting requirements, is a real challenge in mechatronic design. A new multicriteria indicator for mechatronic system performance evaluation is formulated and described in this paper. An example of mechatronic system design is considered to validate the presented approach. It will be shown that the new indicator helps designers to evaluate and therefore optimize a mechatronic system design efficiently and rapidly.},
author = {Hammadi, M. and Choley, J. Y. and Penas, O. and Riviere, a. and Louati, J. and Haddar, M.},
doi = {10.1109/MECATRONICS.2012.6451041},
file = {:home/alexander/Downloads/A new multi-criteria indicator for mechatronic system performance evaluation in preliminary design level.pdf:pdf},
isbn = {9781467347716},
journal = {2012 9th France-Japan and 7th Europe-Asia Congress on Mechatronics, MECATRONICS 2012 / 13th International Workshop on Research and Education in Mechatronics, REM 2012},
keywords = {design evaluation,mechatronic system performance,multi-criteria indicator},
pages = {409--416},
title = {{A new multi-criteria indicator for mechatronic system performance evaluation in preliminary design level}},
year = {2012}
}
@article{Younis2012,
abstract = {The use of approximation models in multiobjective optimization problems that involve expensive analysis and simulation processes such as multi-physics modeling and simulation, finite element analysis (FEA) and computational fluid dynamics (CFD) has become more popular and more attractive, especially for the optimization of complex mechatronics systems. Approximation models have been found as a promising tool for multiobjective optimization problems due to their capability for providing accurate modeling results with much less computations for intensive computation problems. Many present global optimization search techniques involve fitness evaluations that are expensive to perform, even worse for problems with multiple objective black-box functions evaluations. In this work, a new adaptive multiobjective optimization approach based metamodeling (AMOP) techniques is introduced. The approach can identify the Pareto front for multiobjective optimization problems efficiently with high accuracy. The computation cost associated with identifying the Pareto front for expensive black-box functions is reduced. The new search method was tested using benchmark test problems and mechatronics device design examples.},
author = {Younis, Adel and Dong, Zuomin},
doi = {10.1109/MESA.2012.6275565},
file = {:home/alexander/Downloads/Metamodel multi-objective optimization tool for mechatronic system design.pdf:pdf},
isbn = {978-1-4673-2349-9},
journal = {Proceedings of 2012 IEEE/ASME 8th IEEE/ASME International Conference on Mechatronic and Embedded Systems and Applications},
keywords = {AMOP technique,Algorithm design and analysis,Analytical models,Approximation methods,CFD,Computational modeling,FEA,Kriging,Linear programming,Pareto front,Pareto optimisation,Pareto optimization,approximation model,approximation models,approximation theory,black-box functions evaluation,computation cost,computational fluid dynamics,design engineering,finite element analysis,fitness evaluation,global optimization search technique,mechatronic system design,mechatronics,metamodel multiobjective optimization tool,multiobjective optimization,multiphysics modeling,multiphysics simulation,radial basis function,response surface function,search problems},
pages = {218--223},
title = {{Metamodel multi-objective optimization tool for mechatronic system design}},
url = {http://ieeexplore.ieee.org/lpdocs/epic03/wrapper.htm?arnumber=6275565},
year = {2012}
}
